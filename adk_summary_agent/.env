# Required: Set your Google API key for the agent's LLM (Gemini) or Vertex config
GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_GENAI_USE_VERTEXAI=FALSE
# GOOGLE_CLOUD_PROJECT=your-gcp-project-id
# GOOGLE_CLOUD_LOCATION=your-gcp-region

# Required: URL or command for the MCP server hosting Summarization tools
# Example (URL for SSE): SUMMARY_MCP_SERVER_URL=http://<your-mcp-host>:<port>
# Example (Command for Stdio): SUMMARY_MCP_SERVER_URL=npx -y @modelcontextprotocol/server-summarization # Adjust as needed
SUMMARY_MCP_SERVER_URL=your_summary_mcp_server_url_or_command_here

# Required: URL for the LangGraph YouTube Agent A2A endpoint
YOUTUBE_AGENT_A2A_URL=http://localhost:10003 # Default, change if needed

# Optional: Path to service account JSON if MCP server requires auth
# SERVICE_ACCOUNT_JSON_SUMMARY=/path/to/your/summary-sa.json
